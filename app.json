{
  "expo": {
    "name": "storybuilder2-native",
    "slug": "storybuilder2-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "mycoolredirect",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.storybuilder",
      "config":{
        "googleSignIn":{
          "reservedClientId":"com.googleusercontent.apps.584741243002-jmo14kldgqgcl146hvlqdudput99irpg"
        }
      }
    },
    "android": {
      "package": "com.storybuilder",
      "softwareKeyboardLayoutMode":"pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
